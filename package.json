{
  "name": "@sensenet/redux",
  "version": "3.4.3",
  "description": "A set of redux actions, reducers and redux-ovbservable epics for Sense/Net ECM",
  "main": "dist/src/sn-redux.js",
  "scripts": {
    "lint": "node tslint --project ./tsconfig.json",
    "clean": "rimraf dist",
    "precommit": "npm run lint",
    "commit": "git-cz",
    "gulp": "gulp",
    "pretest": "npm run build",
    "test": "nyc mocha dist/test/index.js",
    "check-coverage": "istanbul check-coverage ",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "typedoc:md": "gulp typedoc:md",
    "typedoc:html": "gulp typedoc",
    "semantic-release": "semantic-release pre && semantic-release post",
    "publish:development": "npm publish --tag development"
  },
  "files": [
    "dist",
    "src",
    "test",
    "documentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-redux.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "redux",
    "crud"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-redux/issues"
  },
  "nyc": {
    "cache": true,
    "include": "dist/src/**/*.*",
    "exclude": "dist/src/**/sn-redux.js",
    "all": true,
    "reporter": [
      "lcov",
      "text-summary",
      "json"
    ]
  },
  "homepage": "https://sensenet.com",
  "dependencies": {
    "normalizr": "^3.2.3",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.2",
    "sn-client-auth-google": "^1.0.0",
    "sn-client-js": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^2.2.46",
    "@types/redux-logger": "^3.0.5",
    "chai": "^4.1.1",
    "codecov.io": "^0.1.6",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-typedoc": "^2.0.3",
    "mocha": "^5.0.0",
    "nyc": "^11.1.0",
    "redux-mock-store": "^1.5.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.6.0",
    "typescript": "^2.6.1"
  },
  "czConfig": {
    "path": "node_modules/sensenet-kfi-cz-conventional-changelog"
  },
  "typings": "./src/sn-redux.d.ts"
}
